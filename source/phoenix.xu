  rota_cinta := function:void
     s := string
  begin:
     s<-strcpy base_9 1 1
     base_9<-(strcpy base_9 2 41)+s

     s<-strcpy base_8 41 1
     base_8<-s+(strcpy base_8 1 40)

  end

  pone_nave_madre := function:void
     t := $1:number

  begin:

     color 15
