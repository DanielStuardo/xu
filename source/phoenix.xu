     at ((t+6) 19)
     write     base_8

     color 14
     at ((t+7) 19)
     write     base_7
     at ((t+8) 20)
     write     base_6

     color 12
     at ((t+9) 21)
     write     base_5
     at ((t+10) 22)
     write    base_4

