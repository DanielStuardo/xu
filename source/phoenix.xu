   pone_mensaje(msg:string,linea:number):void
     k:=number
   begin:
      cursor(1)
      color(15)
      for k<-1 to strlen(msg)
        at(linea (35+k)); write strcpy(msg k 1)
        millisec(50)
      next
      cursor(0)
   end

   pone_final_mensaje(msg:string,linea:number):void
     k:=number
   begin:
      cursor(1)
      color(15)
      for k<-1 to strlen(msg)
        at(linea (13+k)); write strcpy(msg k 1)
        millisec(50)
      next
      cursor(0)
   end
   

