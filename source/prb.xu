//#use

//#include

name: <descripcion del programa>

vars:
   n:=number
algorithm:
   for n<-0 to 40000 step 1000
      write n,": ",n%1000,"\n"  /* Esta línea no hace nada
                                   pero refleja el espíritu
                                   de no hacer nada, por tanto,
                                   hace algo */               
   next
   ."\n"
   pause
   for n<-4 to 40000 step 1000
      write n,": ",n%1000,"\n"
   next
/*
   n%1000 + n%1000
     123456789
     123456789 5555
     123456789 559595
     123456789 559595
     123456789 123456789   9 9
*/
stop
