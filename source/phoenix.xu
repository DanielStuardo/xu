  pone_actor := function:void
     x := $1:number
     y := $2:number
     r := $3:number
     c := $4:number
  begin:
     color c
     at y x
     if r=0
        write "  v*v  "
        at ((y+1) x)
        write  " /   \ " 
     elseif r=1
        write "       "
        at ((y+1) x)
        write  "--v*v--"
     else
        write " \   / "
        at ((y+1) x)
        write  "  v*v  "
     endif
     color 7
  end

  saca_actor := function:void  // ver si sirve mejor escribir espacios en pone_actor
     x :=  $1:number
     y :=  $2:number
  begin:
     at y x
     write  "        "
     at ((y+1) x)
     write  "        "
  end

