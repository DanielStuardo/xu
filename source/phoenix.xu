        at ((t+3) 38)
        write    "■"
        color 12
        at ((t+3) 39)
        write    "█"
        color 10
        at ((t+3) 40)
        write    "■"
        titila<-1
     else
        color 3
        at ((t+3) 38)
        write    "■"
        color 4
        at ((t+3) 39)
