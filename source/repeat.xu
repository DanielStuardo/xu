vars:
  i := number
algorithm:
  cls
  at(2 1)
  i<-1
  ."Mensaje tabeado..."
  do
     write htab(3) i   // imprime en la posicion 24
     htab(-1)   // vuelve al principio del numero
     ++i
  until i>50000
 ."\n"
 
  i<-1
  at(4 1)
  ."Mensaje tabeado..."
  do
     write at(4 19) i   // imprime en la posicion 24
     ++i
  until i>50000
stop